    if Tint[1] == Tint2 then startLuaCoroutine(self, 'DeleteWhite') end
    if Tint[2] == Tint2 then startLuaCoroutine(self, 'DeletePink') end
    if Tint[3] == Tint2 then startLuaCoroutine(self, 'DeleteGreen') end
