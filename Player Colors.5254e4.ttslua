    if Tint[6] == Tint2 then startLuaCoroutine(self, 'DeleteYellow') end
end

function DeleteWhite()
    Food = {'55b456', 'd1a24f', '360bde', 'ce1b29', '6d3be7', 'cf8d16', '32ef37'}
    for i=1, 7, 1 do
    Haversack.putObject(getObjectFromGUID(Food[i]))
    end
    wait(0.4)

    Zone = getObjectFromGUID('e09210')
    local obj = {}
    obj = Zone.getObjects()
    for i=1, #obj, 1 do
    obj[i].destruct()
    end

    return 1
end

